  journaltitle = {Journal of Neurophysiology},
  keywords = {Action Potentials,Action Potentials: physiology,Animals,clean_citation,Locus Coeruleus,Locus Coeruleus: physiology,Macaca fascicularis,Male,Neurons,Neurons: physiology,paper_pass,Photic Stimulation,Photic Stimulation: methods,Psychomotor Performance,Psychomotor Performance: physiology},
